@using Travel_Trip_Project.Models.Sınıflar
@model List<Blog>
<div class="box1-left">
    @foreach (var x in Model)
    {
    <div class="bot-img">
        <img src="@x.BlogImage" alt="" />
    </div>
    <div class="bot-desc1">
        <p><a href="/Blog/BlogDetay/@x.ID">@x.Aciklama.Substring(0, 80)...</a></p>
    </div>
    <div class="clear"></div>
    }
</div>